@InProceedings{EngelmannVasishth2009,
  author        = {Engelmann, Felix and Vasishth, Shravan},
  title         = {{Processing grammatical and ungrammatical center embeddings in English and German: A computational model}},
  booktitle     = {{Proceedings of 9th International Conference on Cognitive Modeling, ICCM}},
