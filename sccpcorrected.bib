journal = {Memory \& Cognition},
volume = {41},
number = {1},
pages = {82--97},
publisher={Springer-Verlag},
keywords = {eye movements,reading,regressions},
pmid = {22886737},
title = {{The function of regressions in reading: Backward eye movements allow rereading.}},
year = {2013}
}


