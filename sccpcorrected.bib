@InProceedings{EngelmannVasishthEngbert2013a,
  author        = {Engelmann, F. and Vasishth, S. and Engbert, R. and Kliegl, R.},
  title         = {{A cognitive model interfacing eye movements with parsing}},
  booktitle     = {{Book of Abstracts of the 17th European Conference on Eye Movements}},
  year          = {2013},
  editor        = {Holmqvist, K. and Mulvey, F. and Johansson, R.},
  address       = {Lund, Sweden},
  publisher     = {Journal of Eye Movement Research, 6(3)},
  abstract      = {Reichle et al. (Psychonomic Bulletin {\&} Review, 2009) proposed two kinds of interaction between post-lexical processing and oculomotor control: (1) "slow integration failure": A time-out regression when post-lexical integration is lagged; and (2) "rapid integration failure": A regression when integration fails. Boston et al. (Eye Movement Research, 2008; Language and Cognitive Processes, 2011) showed that memory retrieval (Lewis {\&} Vasishth, Cognitive Science, 2005) and syntactic surprisal (Hale, NAACL, 2001; Levy, Cognition, 2008) are significant predictors of eye movements in the Potsdam Sentence Corpus. We implemented the proposed interaction of the first kind in the cognitive architecture ACT-R and evaluated the model on the Potsdam Sentence Corpus using pre-calculated values from Boston et al. (2008, 2011). The model's prediction of 11 fixation measures including second-pass durations and regression rates was grouped by classes of word frequency and fitted to the data with a resulting root-mean-square error of 0.206 and a correlation R of 0.9. Based on this evaluation we present work in progress that uses the parser of Lewis {\&} Vasishth (2005) for a direct interaction of eye movement control with parsing rules and implements rapid integration failure resulting in long-range regressions guided by active repair attempts.},
  bdsk-file-1   = {},
  date-added    = {2018-01-22 18:16:54 +0000},
  date-modified = {2018-01-22 18:16:54 +0000},
  journal       = {Journal of Eye Movement Research},
  read          = {1},
}

