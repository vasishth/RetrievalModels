@article{vasishthysall04,
	Abstract = {Discourse context has been argued to be the main factor responsible for increased processing difficulty in non-canonical order sentences: if appropriate discourse context is provided (the argument goes) both canonical and non-canonical order sentences are equally easy to process. This research suggests that this generalization may not be true across languages: the distance between arguments and verbs could affect the ease with which the former can be integrated with the latter, and sufficiently increasing this distance makes processing difficult, regardless of discourse context.},
