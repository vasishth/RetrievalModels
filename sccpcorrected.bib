@Article{Jaeger2008,
  author   = {Jaeger, T. F. and Fedorenko, E. and Gibson, E.},
  title    = {{Anti-locality in English: Consequences for Theories of Sentence Comprehension}},
  journal  = {submitted},
  year     = {2008},
  pages    = {1--44},
  abstract = {In English, integration of an argument with its head generally leads to slower comprehension at the head (e.g., verb), the more material intervenes between the two. Such â€œlocalityâ€ effects have been interpreted as evidence for memory-based comprehension accounts. Most memory-based comprehension accounts are, however, challenged by cases where more intervening material facilitates processing. Such â€œanti-localityâ€ effects are often interpreted as evidence for expectation-based processing. An alternative interpretation of anti-locality effects within a memory-based account is proposed in (Lewis, Vasishth, \& Van Dyke, 2006): intervening material can lead to repeated retrieval and hence reactivation of dependents. This reactivation can counteract activation-decay and lead to faster retrieval of the dependents at the head. We present new evidence of anti-locality from English relative clauses that existing memory-based accounts, including Lewis et al., 2006, cannot explain. We argue that memory-based and expectation-based models need to be integrated in order to account for the existence of locality and anti-locality effects. We also introduce a two-step mixed effect analysis of reading time data that incorporates controls for several potential confounds common in experiments that investigate locality and anti-locality effects.},
  address  = {Brain and Cognitive Sciences, University of Rochester; Brain and Cognitive Sciences, MIT},
  file     = {:Users/felx/Dropbox/Mendeley Papers/Jaeger, Fedorenko, Gibson/Jaeger, Fedorenko, Gibson - 2008 - Anti-locality in English Consequences for Theories of Sentence Comprehension - submitted.pdf:pdf},
}


