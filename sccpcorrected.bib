	Pages = {149--168},
	Pmid = {9468769},
	Title = {{On-line processing of filler-gap construction in aphasia}},
	Volume = {61},
	Year = {1998},
	Bdsk-Url-1 = {http://www.hubmed.org/display.cgi?uids=9468769}}

